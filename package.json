{
  "name": "byerun-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "install-all": "npm install && cd api && npm install",
    "serve": "vite",
    "build": "vite build",
    "api": "node api/index.js",
    "dev": "concurrently \"npm run api\" \"npm run serve\"",
    "lint": "eslint --ext .js,.vue src",
    "deploy": "gh-pages -d dist",
    "start": "NODE_ENV=development npm run dev",
    "build:prod": "NODE_ENV=production npm run build"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "axios": "^1.6.8",
    "core-js": "^3.8.3",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.7.2",
    "geolib": "^3.3.4",
    "js-md5": "^0.8.3",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "pinia": "^2.2.4",
    "query-string": "^9.0.0",
    "querystring-es3": "^0.2.1",
    "random": "^4.1.0",
    "vite": "^5.4.10",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.3.2",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.25.9",
    "@types/crypto-js": "^4.2.2",
    "@types/md5": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "concurrently": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gh-pages": "^6.2.0",
    "push-dir": "^0.4.1",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}